#include <iostream>
using namespace std;
bool isPalindrome( int x)
{   int m,reverse=0,n=x;
    while(x!=0)
    {    
        m=x%10;
        reverse=reverse*10+m;
      x=x/10;
       
       
    }
    if(n==reverse)
  { return true;
  }
    else return false;
}

int main()
{
   isPalindrome(11);

     if(isPalindrome(2121))
     {
     cout<<" Yes!";
     }
     else cout<<"No";
   

}

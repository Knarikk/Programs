
#include <iostream>
#include<vector>
using namespace std;
  vector<char>arr1={'a','b','c','d','e'};
    vector<char>arr2={'b','c','d','e','a'};
    void nkod (char*e)
    { 
        while ((*e))
        {
         int i;
         for(i=0;i<arr1.size();i++)

             if((*e)==arr1[i])  
            {
             *e=arr2[i];
               break;
            }
        e++;

        }
    }
   void dkod()
   {

   }
int main()
{
  char str[20];
  cout<<"Enter string\n";
  cin>>str;
  nkod(str);
  cout<<str;
  return 0;
}

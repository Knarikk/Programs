#include <iostream>
using namespace std;
 

   void ԼargestDivisor(int n)
   { 
         int m;int i;
           
              for(int i=n/2;i>=1;i--)
            {
               if(n%i==0)
               {
                 m=i;
               
                break;
               }
            
            }
              cout<<m;
            
   }

int main()
{  
   ԼargestDivisor(36);
    return 0;
}

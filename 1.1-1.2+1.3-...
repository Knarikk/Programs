// 1.1-1.2+1.3-â€¦


#include <iostream>
using namespace std;

 float sumNum(int n)
   
   {  
      float sum=0;
       float k=1.1;
        int t=1;
      for (int i=1;i<=n;i++)
       {  
         sum=sum+(k*t);
           k=k+0.1;
            t=t*(-1);
            
       }
       
       return sum;
     
    }
   
  int main()
{
 
cout<<sumNum(4);
    return 0;
}

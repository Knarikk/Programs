#include <iostream>
#include <vector>
using namespace std;



  void BubbleSort  ( vector<int> & v)
  { int temp;
    for(int i=0;i<v.size()-1;i++)
    {
        for(int j=i+1;j<v.size();j++)
        {
          if(v[j]<v[i])
          {   temp=v[i];
              v[i] = v[j];
              v[j] = temp;
              
          }
        }
    }
  }
 
int main()
   
{
    vector<int>v  = {4,2,8,6,10};
       BubbleSort(v);
    for (unsigned i = 0; i < v.size(); ++i) 
    {
        std::cout << v[i] << " ";
    }
   
    
    return 0;
}
